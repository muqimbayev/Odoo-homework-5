<odoo>

    <record id="clinic_work_schedule_views_list" model="ir.ui.view">
        <field name="name">clinic.work_schedule.list</field>
        <field name="model">clinic.work_schedule</field>
        <field name="arch" type="xml">
            <list>

                    <field name="week_days"/>
                    <field name="working_hours"/>
                    <field name="lunch_time"/>
            </list>
        </field>
    </record>

    <record id="clinic_work_schedule_views_form" model="ir.ui.view">

    <field name="name">clinic.work_schedule.form</field>
    <field name="model">clinic.work_schedule</field>
    <field name="arch" type="xml">
        <form>
        <sheet>
            <group>
                <field name="week_days"/>
                <field name="working_hours"/>
                <field name="lunch_time"/>
            </group>
            </sheet>
        </form>
    </field>
    </record>
    
    <record id="clinic_work_schedule_action" model="ir.actions.act_window">
        <field name="name">Ish jadvali</field>
        <field name="res_model">clinic.work_schedule</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="clinic_menu" name="Klinika"/>
    <menuitem id="clinic_menu_subdomain_work_schedule" name="Ish jadvali" parent="clinic_menu" action="clinic_work_schedule_action"/>

</odoo>