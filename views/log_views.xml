<odoo>
<record id="log_module_log_views_list" model="ir.ui.view">
  <field name="name">log_module.log.list</field>
  <field name="model">log_module.log</field>
  <field name="arch" type="xml">
  <list>
        <field name="username"/>
        <field name="message"/>
        </list>
  </field>
</record>

<record id="log_module_log_views_form" model="ir.ui.view">
  <field name="name">log_module.log.form</field>
  <field name="model">log_module.log</field>
  <field name="arch" type="xml">
    <form>
      <sheet>
      <group>
        <field name="username"/>
        <field name="message"/>
        </group>
      </sheet>
      </form>
  </field>
</record>

<record id="log_module_log_action" model="ir.actions.act_window">
  <field name="name">Loglar</field>
  <field name="res_model">log_module.log</field>
  <field name="view_mode">list,form</field>

</record>

<menuitem id="log_module_submenu_log" name="Loglar" parent="log_module_menu" action="log_module_log_action"/>

</odoo>