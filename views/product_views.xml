<odoo>
<record id="log_module_product_views_list" model="ir.ui.view">
  <field name="name">log_module.product.list</field>
  <field name="model">log_module.product</field>
  <field name="arch" type="xml">
  <list>
        <field name="name"/>
        <field name="price"/>
        <field name="quantity"/>
        </list>
  </field>
</record>

<record id="log_module_product_views_form" model="ir.ui.view">
  <field name="name">log_module.product.form</field>
  <field name="model">log_module.product</field>
  <field name="arch" type="xml">
    <form>
      <sheet>
      <group>
        <field name="name"/>
        <field name="price"/>
        <field name="quantity"/>
      </group>
      </sheet>
    </form>
  </field>
</record>

<record id="log_module_product_action" model="ir.actions.act_window">
  <field name="name">Mahsulotlar</field>
  <field name="res_model">log_module.product</field>
  <field name="view_mode">list,form</field>

</record>

<menuitem id="log_module_menu" name="Log dasturi"/>
<menuitem id="log_module_submenu_product" name="Mahsulotlar" parent="log_module_menu" action="log_module_product_action"/>

</odoo>